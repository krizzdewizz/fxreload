<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.web.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane fx:id="root" prefHeight="523.0" prefWidth="652.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fxreload.MainController">
   <center>
      <TabPane fx:id="tabPane" />
   </center>
   <top>
      <HBox spacing="5.0" BorderPane.alignment="CENTER">
         <children>
            <MenuButton fx:id="favMenu" onAction="#onOpen" text="File" />
            <ToggleButton fx:id="onTop" mnemonicParsing="false" onAction="#onTop" text="On Top" />
            <Button mnemonicParsing="false" onAction="#onReloadNow" text="Reload Now" />
            <Label fx:id="msg" alignment="CENTER_RIGHT" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS" />
         </children>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </HBox>
  
   </top>
</BorderPane>
